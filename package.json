{
  "name": "things-scene-chartjs",
  "version": "2.0.0",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/index.js -o things-scene-chartjs.js src/index.js & polymer serve",
    "build": "browserify src/index.js -o things-scene-chartjs.js && uglifyjs things-scene-chartjs.js -m -c -o things-scene-chartjs-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-chartjs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-chartjs/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-chartjs#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1"
  },
  "dependencies": {
    "bower-npm-resolver": "^0.4.0"
  }
}
